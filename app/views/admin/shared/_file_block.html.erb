<div class="file-block">
  <div class="block preview">
    <p><span class="icon icon-picture-o">&nbsp;</span></p>
    <p class="image_public_id">Click or drag image here</p>
    <p class="small">( maximum size: 10mb )</p>
  </div>
  <div class="progress hide">
    <div class="progress-bar" role="progressbar" aria-valuenow="70"
    aria-valuemin="0" aria-valuemax="100" style="width:0%">
      <span class="sr-only">0% Complete</span>
    </div>
  </div>

  <% if f.object.main_image_hashed_id.present? %>
    <%= cl_image_tag(f.object.main_image_hashed_id, width: 100, height: 100, crop: :fill) %>
  <% end %>
  <%= cl_image_upload_tag(block_name) %>
</div>
<ul class="parsley-errors-list js-cloudinary-error filled hide"></ul>
